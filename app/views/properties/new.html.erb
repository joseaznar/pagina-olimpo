<%= form_for @property do |form| %>
	<%= form.label :nombre %>
	<%= form.text_field :nombre %>
	<%= form.label :descripcion %>
	<%= form.text_area :descripcion %>
	<%= form.label :metros_cuadrados %>
	<%= form.text_field :metros_cuadrados %>
	<%= form.label :direccion %>
	<%= form.text_field :direccion %>
	<%= form.label :renta %>
	<%= form.check_box :renta %>
	<%= form.label :precio_renta %>
	<%= form.text_field :precio_renta %>
	<%= form.label :venta %>
	<%= form.check_box :venta %>
	<%= form.label :precio_venta %>
	<%= form.text_field :precio_venta %>	
	
	<%= form.submit "submit" %>
<% end %>